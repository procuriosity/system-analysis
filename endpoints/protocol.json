{
  "openapi": "3.0.0",
  "info": {
    "title": "Protocol",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "{host}"
    }
  ],
  "paths": {
    "/create/protocol/{Contract Process ID}/{Lot Identifier}": {
      "post": {
        "summary": "Protocol",
        "operationId": "pcrProtocol",
        "description": "",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "Authorization Token ",
            "description": "Bearer {access_token}",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "Transaction ID",
            "description": "ID транзакции",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "Entity Token ",
            "description": "tender токен",
            "required": true
          }
        ],
        "responses": {
          "202": {
            "description": "Accepted",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "x-examples": {
                    "Example 1": {
                      "contracts": [
                        {
                          "id": "3fa85f64-5717-4562-b3fc-2c963f66afa6",
                          "date": "2022-12-28T12:13:38.416Z",
                          "status": "pending",
                          "statusDetails": "contractProject",
                          "awardId": "string",
                          "lotId": "string"
                        }
                      ]
                    }
                  },
                  "properties": {
                    "contracts": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "format": "uuid"
                          },
                          "date": {
                            "type": "string",
                            "format": "date-time"
                          },
                          "status": {
                            "type": "string",
                            "enum": [
                              "pending"
                            ]
                          },
                          "statusDetails": {
                            "type": "string",
                            "enum": [
                              "contractProject",
                              "unsuccessful"
                            ]
                          },
                          "awardId": {
                            "type": "string"
                          },
                          "lotId": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "id",
                          "date",
                          "status",
                          "statusDetails",
                          "lotId"
                        ]
                      }
                    },
                    "tender": {
                      "type": "object",
                      "required": [
                        "lots"
                      ],
                      "properties": {
                        "lots": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string"
                              },
                              "status": {
                                "type": "string",
                                "enum": [
                                  "active"
                                ]
                              },
                              "statusDetails": {
                                "type": "string",
                                "enum": [
                                  "awarded"
                                ]
                              }
                            },
                            "required": [
                              "id",
                              "status",
                              "statusDetails"
                            ]
                          }
                        }
                      }
                    }
                  },
                  "required": [
                    "contracts",
                    "tender"
                  ]
                },
                "examples": {}
              }
            }
          }
        }
      },
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "Contract Process ID",
          "in": "path",
          "required": true,
          "description": "cpid/ocid tender"
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "Lot Identifier",
          "in": "path",
          "required": true,
          "description": "lot id"
        }
      ]
    }
  },
  "components": {
    "schemas": {}
  }
}